Dialog>\n  );\n}\n\nexport default forwardRef(CharacterEpisodesDialog);\n","/Users/claudio/Workspace/cloud-academy/rick-n-morty/src/index.tsx",[],"/Users/claudio/Workspace/cloud-academy/rick-n-morty/src/redux/slices/episodeSlice.ts",["63","64"],"/Users/claudio/Workspace/cloud-academy/rick-n-morty/src/reportWebVitals.ts",[],{"ruleId":"65","severity":1,"message":"66","line":1,"column":9,"nodeType":"67","messageId":"68","endLine":1,"endColumn":18},{"ruleId":"65","severity":1,"message":"69","line":1,"column":20,"nodeType":"67","messageId":"68","endLine":1,"endColumn":30},{"ruleId":"70","severity":1,"message":"71","line":20,"column":6,"nodeType":"72","endLine":20,"endColumn":24,"suggestions":"73"},{"ruleId":"65","severity":1,"message":"74","line":1,"column":24,"nodeType":"67","messageId":"68","endLine":1,"endColumn":32},{"ruleId":"75","severity":1,"message":"76","line":52,"column":46,"nodeType":"77","endLine":52,"endColumn":61},{"ruleId":"75","severity":1,"message":"76","line":63,"column":48,"nodeType":"77","endLine":63,"endColumn":63},{"ruleId":"70","severity":1,"message":"78","line":22,"column":6,"nodeType":"72","endLine":22,"endColumn":16,"suggestions":"79"},{"ruleId":"65","severity":1,"message":"80","line":1,"column":40,"nodeType":"67","messageId":"68","endLine":1,"endColumn":53},{"ruleId":"65","severity":1,"message":"81","line":2,"column":9,"nodeType":"67","messageId":"68","endLine":2,"endColumn":23},"@typescript-eslint/no-unused-vars","'AnyAction' is defined but never used.","Identifier","unusedVar","'AsyncThunk' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'page'. Either include it or remove the dependency array.","ArrayExpression",["82"],"'useState' is defined but never used.","react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" is a security risk: see https://html.spec.whatwg.org/multipage/links.html#link-type-noopener","JSXAttribute","React Hook useEffect has a missing depen